  <option name="HTML_ALIGN_TEXT" value="true" />
  <option name="HTML_ELEMENTS_TO_INSERT_NEW_LINE_BEFORE" value="body,div,p,form,h1,h2,h3,img,input,select" />
  <option name="HTML_INLINE_ELEMENTS" value="abbr,acronym,b,basefont,bdo,big,br,cite,cite,code,dfn,em,font,i,kbd,label,q,s,samp,span,strike,strong,sub,sup,textarea,tt,u,var" />
